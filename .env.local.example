# LiveKit server URL (used server-side in token generation)
LIVEKIT_URL=wss://your-project.livekit.cloud

# LiveKit API credentials (server-side only — never expose to client)
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# LiveKit public URL (inlined at build time for the client)
# Must match LIVEKIT_URL — copy the same value here
NEXT_PUBLIC_LIVEKIT_URL=wss://your-project.livekit.cloud
